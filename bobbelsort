#include<stdio.h>
#define SIZE 5
void bobblesort(int[]);

int main()
{
int arr[SIZE]={5,3,4,2,1};
printf("before sorting : ");



for(int k = 0; k < SIZE; k++ ){
    printf("%d",arr[k]);
}

bubbelsort (arr);
printf("\n\n after sorting :");
for (int k =0; k<SIZE;k ++){
    printf("%d",arr[k]);

}
printf("\n");
}
void bobblesort(int arr[]){
 for (i=0;i<SIZE;i++){
  for (int j=0;j<SIZE;j++){{
  if (arr[i]<arr[j]){
   int temp=arr[i];
    arr[i]=arr[j];
    arr[j]=temp;
    }
    }
 }

}
